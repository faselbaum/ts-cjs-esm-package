{
    "name": "ts-cjs-esm-package",
    "version": "1.0.0",
    "description": "A package shipping as CJS and ESM",
    "main": "dist/index.js",
    "module": "dist/__esm__/index.mjs",
    "types": "dist/__types__/index.d.ts",
    "files": [
      "dist/**/*"
    ],
    "exports": {
      ".": {
        "types": "./dist/__types__/index.d.ts",
        "import": "./dist/__esm__/index.mjs",
        "require": "./dist/index.js"
      },
      "./*": {
        "types": "./dist/__types__/*.d.ts",
        "import": "./dist/__esm__/*.mjs",
        "require": "./dist/*.js"
      },
      "./package.json": "./package.json"
    },
    "typesVersions": {
      "*": {
        "dist/__types__/index.d.ts": ["./dist/__types__/index.d.ts"],
        "*": ["./dist/__types__/*.d.ts", "./dist/__types__/*/index.d.ts"]
      }
    },
    "scripts": {
      "build:esm": "tsc --project tsconfig.esm.json && npm run build:esm:fix-relatives && npm run build:esm:remap-cjs-dependencies",
      "build:cjs": "tsc --project tsconfig.cjs.json",
      "prebuild": "rm -rf ./dist",
      "build": "rm -rf ./dist && run-p build:cjs build:esm",
      "build:esm:fix-relatives": "tsc-esm-fix --tsconfig='./tsconfig.esm.json' --ext='.mjs' --sourceMap",
      "build:esm:remap-cjs-dependencies": "babel --config-file ./babel.esm.config.json ./dist/__esm__/ -d ./dist/__esm__ --keep-file-extension"
    },
    "dependencies": {
      "lodash": "4.17.21",
      "lodash-es": "4.17.21",
      "styled-components": "5.3.5"
    },
    "devDependencies": {
      "@babel/core": "7.18.13",
      "@babel/cli": "7.18.10",
      "babel-plugin-module-resolver": "4.1.0",
      "@types/lodash": "4.14.184",
      "tsc-esm-fix": "2.20.4",
      "typescript": "~4.7.2",
      "@types/styled-components": "5.1.26"
    },
    "author": "",
    "license": "MIT",
    "sideEffects": false
  }
  